{
    "compilerOptions": {
        "target": "es5",
        "module": "esnext",
        "lib": ["es2015", "es2016", "es2017","es2018","es2019","es2020","dom", "es2018.promise"],
        "jsx": "react",
        "noEmit": true,
        "sourceMap": true,
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "allowSyntheticDefaultImports": true
    },
    "include": [
        "src",
        "images.d.ts"
    ],
    "typeRoots": [
        "./node_modules/@types",
        "./@types"
      ]
}